{
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "C_Cpp.intelliSenseEngine": "disabled",
    "cppref.searchEngine": "DuckDuckGo",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorSurroundingLines": 3,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontFamily": "'IosevkaTerm Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.indentation": false,
    "editor.inlayHints.enabled": "off",
    "editor.lineNumbers": "off",
    "editor.minimap.enabled": false,
    "editor.occurrencesHighlight": "singleFile",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollBeyondLastLine": false,
    "editor.snippetSuggestions": "top",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordWrapColumn": 120,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "files.trimTrailingWhitespace": true,
    "prettier.tabWidth": 4,
    "security.workspace.trust.untrustedFiles": "open",
    "settings._comment": "The ID should be unique for each setting",
    "settings.cycle": [
        {
            "id": "editor-guides-bracketPairs",
            "overrideWorkspaceSettings": true,
            "values": [
                { "editor.guides.bracketPairs": "active" },
                { "editor.guides.bracketPairs": false }
            ]
        },
        {
            "id": "editor-guides-indentation",
            "overrideWorkspaceSettings": true,
            "values": [
                { "editor.guides.indentation": true },
                { "editor.guides.indentation": false }
            ]
        },
        {
            "id": "editor-lineNumbers",
            "overrideWorkspaceSettings": true,
            "values": [
                { "editor.lineNumbers": "relative" },
                { "editor.lineNumbers": "off" },
                { "editor.lineNumbers": "on" }
            ]
        },
        {
            "id": "editor-occurrencesHighlight",
            "overrideWorkspaceSettings": true,
            "values": [
                { "editor.occurrencesHighlight": "singleFile" },
                { "editor.occurrencesHighlight": "off" }
            ]
        }
    ],
    "task.allowAutomaticTasks": "on",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontFamily": "Iosevka Term Slab",
    "terminal.integrated.lineHeight": 1.1,
    "vscode-neovim.logOutputToConsole": true,
    "vsicons.dontShowNewVersionMessage": true,
    "window.commandCenter": true,
    "window.customMenuBarAltFocus": false,
    "window.customTitleBarVisibility": "auto",
    "window.enableMenuBarMnemonics": false,
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Catppuccin Frapp√©",
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.showTabs": "single",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": false,
    "workbench.activityBar.location": "top",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "workbench.externalBrowser": "/tool/pandora64/.package/firefox-117.0.1/bin/firefox"
}
